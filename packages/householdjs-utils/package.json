{
	"name": "@householdjs/utils",
	"version": "2.1.2-alpha.0",
	"description": "style utils to make you worry less about styling and more about application logic",
	"keywords": [
		"css",
		"utils",
		"style",
		"utils",
		"styling"
	],
	"author": "Daniel Zeman <ledaniel.zeman@gmail.com>",
	"homepage": "https://github.com/LeDanielH/HouseholdJS#readme",
	"license": "ISC",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"directories": {
		"lib": "lib",
		"test": "__tests__"
	},
	"files": [
		"lib"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LeDanielH/HouseholdJS.git"
	},
	"scripts": {
		"build": "npm run clean && npm run tsc",
		"clean": "npm run clean:lib && npm run clean:tsbuildinfo",
		"tsc": "tsc -b tsconfig.json",
		"clean:lib": "rimraf lib",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo",
		"upgrade": "ncu -u",
		"fix:packages": "npm audit fix"
	},
	"bugs": {
		"url": "https://github.com/LeDanielH/HouseholdJS/issues"
	},
	"dependencies": {
		"@householdjs/types": "^2.1.2-alpha.0",
		"react-dom": "^16.13.1",
		"styled-components": "^5.1.1",
		"ts-optchain": "^0.1.8"
	},
	"devDependencies": {
		"typescript": "^4.0.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
