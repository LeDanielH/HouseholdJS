{
	"name": "@householdjs/ui",
	"version": "1.2.0",
	"description": "premade unstyled ui elements",
	"keywords": [
		"ui",
		"elements",
		"ui",
		"elements"
	],
	"author": "Daniel Zeman <ledaniel.zeman@gmail.com>",
	"homepage": "https://github.com/LeDanielH/HouseholdJS#readme",
	"license": "ISC",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"directories": {
		"lib": "lib",
		"test": "__tests__"
	},
	"files": [
		"lib"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LeDanielH/HouseholdJS.git"
	},
	"scripts": {
		"build": "npm run clean && npm run tsc",
		"clean": "npm run clean:lib && npm run clean:tsbuildinfo && npm run clean:docs",
		"tsc": "tsc -b tsconfig.json",
		"clean:docs": "rimraf docs",
		"clean:lib": "rimraf lib",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo",
		"docs": "npm run clean:docs & npm run docs:generate",
		"docs:generate": "typedoc --out docs/ src/ --mode file --readme none --theme minimal",
		"upgrade": "ncu -u"
	},
	"bugs": {
		"url": "https://github.com/LeDanielH/HouseholdJS/issues"
	},
	"dependencies": {
		"@householdjs/utils": "^1.2.0",
		"householdjs": "^1.2.0",
		"react": "^16.10.2",
		"styled-components": "^4.4.0"
	},
	"devDependencies": {
		"@types/react": "^16.9.5",
		"@types/styled-components": "^4.1.19",
		"typescript": "^3.6.3"
	},
	"publishConfig": {
		"access": "public"
	}
}
