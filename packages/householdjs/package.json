		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo",
		"upgrade": "ncu -u",
		"fix:packages": "npm audit fix"
	},
	"bugs": {
		"url": "https://github.com/LeDanielH/HouseholdJS/issues"
	},
	"dependencies": {
		"@householdjs/elements": "^2.1.2-alpha.0",
