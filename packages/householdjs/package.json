		"@householdjs/ui": "^2.1.2-alpha.0",
		"@householdjs/utils": "^2.1.2-alpha.0",
		"ts-optchain": "^0.1.8"
	},
	"devDependencies": {
		"typescript": "^4.0.2"
	},
	"publishConfig": {
		"access": "public"
	}
