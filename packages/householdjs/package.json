{
	"name": "householdjs",
	"version": "2.1.2-alpha.0",
	"description": "householdjs ui elements library",
	"keywords": [
		"layout",
		"styled-components"
	],
	"author": "Daniel Zeman <ledaniel.zeman@gmail.com>",
	"homepage": "https://github.com/LeDanielH/HouseholdJS#readme",
	"license": "ISC",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"directories": {
		"lib": "lib",
		"test": "__tests__"
	},
	"files": [
		"lib"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LeDanielH/HouseholdJS.git"
	},
	"scripts": {
		"build": "npm run clean && npm run tsc",
		"clean": "npm run clean:lib && npm run clean:tsbuildinfo",
		"tsc": "tsc -b tsconfig.json",
