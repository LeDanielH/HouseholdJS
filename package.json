{
	"name": "householdjs-monorepo",
	"version": "1.3.1",
	"private": true,
	"devDependencies": {
		"@types/jest": "^26.0.3",
		"@types/node": "^14.0.14",
		"jest": "^26.1.0",
		"lerna": "^3.22.1",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.1.1",
		"ts-optchain": "^0.1.8",
